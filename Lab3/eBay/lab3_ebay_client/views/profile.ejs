			<br><br>
			<div class="row"> 
				<div class="col-sm-2"> </div> <!--left empty column-->
				
				<div class="col-sm-8"> 
					<h2>Start selling on eBay today    <a style="float:right" ng-click="createad()">Create a new ad</a></h2>
					<div><hr></div>
					
					<div class="row">
					<div class="col-sm-2"> </div>
					
					<div class="col-sm-8"> 
						<div class="panel panel-default" ng-show="createanad">
							<!-- Default panel contents -->
							<div class="panel-heading" align="middle">
							<strong><h3>New advertisement</h3></strong>
							<ul class="nav nav-tabs" >
								<li class="active" style="width: 50%;font-size:auto;">
									<a data-toggle="tab" href="#normalproduct"><strong><h4>Normal</h4></strong></a>
								</li>
								<li style="width: 50%;font-size:auto;">
									<a data-toggle="tab" href="#bidproduct"><strong><h4>Bid</h4></strong></a>
								</li>
							</ul>
							
							
							</div>
								
								<div class="panel-body">
								<div class="tab-content">
								
								<div id="normalproduct" class="tab-pane fade in active">
									<form class="form-horizontal" role="form">
						
										<div class="form-group usernameDiv">
											<!--label class="inputUsername" class="col-sm-2 control-label pull-left">Enter your Email id</label-->
											<div class="col-sm-12">
												<input type="text" class="form-control" name="inputUsername" id="inputUsername" ng-model='Product_title' placeholder="Product title&#42;">
											</div>
										</div>
										
										<div class="form-group usernameDiv">
											<div class="col-sm-12">
												<input type="text" class="form-control" name="inputUsername" id="inputUsername" ng-model='Product_description' placeholder="Product description&#42;">
											</div>
										</div>
										
										<div class="form-group usernameDiv">
											<div class="col-sm-6">
												<input type="number" class="form-control" name="inputUsername" id="inputUsername" ng-model='Available_quantity' placeholder="Available quantiy&#42;">
											</div>
											
											<div class="col-sm-6">
												<input type="number" class="form-control" name="inputUsername" id="inputUsername" ng-model='Product_price' placeholder="Product price&#42;">
											</div>
										</div>
						

										<div class="form-group ">
											<div align='middle'>
												<button  class="btn btn-success" ng-click="adsubmit()">Done</button>
											</div>
										</div>
									</form>
								</div>	
								
								
								<div id="bidproduct" class="tab-pane fade">
									<form class="form-horizontal" role="form">
						
										<div class="form-group usernameDiv">
											<div class="col-sm-12">
												<input type="text" class="form-control" ng-model='bProduct_title' placeholder="Product title&#42;">
											</div>
										</div>
										
										<div class="form-group usernameDiv">
											<div class="col-sm-12">
												<input type="text" class="form-control" ng-model='bProduct_description' placeholder="Product description&#42;">
											</div>
										</div>
										
										<div class="form-group usernameDiv">
											<div class="col-sm-6">
												<input type="number" class="form-control" ng-model='bAvailable_quantity' placeholder="Available quantiy&#42;">
											</div>
											
											<div class="col-sm-6">
												<input type="number" class="form-control" ng-model='bProduct_price' placeholder="Bidding price&#42;">
											</div>
										</div>
										
										<div class="form-group usernameDiv">
											<div class="col-sm-6">
												<input type="number" class="form-control" ng-model='bNoofdays' placeholder="Bidding days&#42;">
											</div>
											
											<div class="col-sm-6">
									
											</div>
										</div>
						

										<div class="form-group ">
											<div align='middle'>
												<button  class="btn btn-success" ng-click="badsubmit()">Done</button>
											</div>
										</div>
									</form>
								</div>								
								
								
								</div>
								</div>
								
						
				
					</div>
					<div class="col-sm-2"> </div>
					</div>
					
				</div>
	
				<div class="col-sm-2"> </div> <!--right empty column-->
			</div>
			</div>
			
			
			
		
			<div class="row"> <!--FIFTH MAIN ROW OR LAST ROW for Today's Featured Collections-->
				<div class="col-sm-2"> </div> <!--left empty column-->
				
				<div class="col-sm-8"> 
					<h2>Items for sale ({{resultlength}})<a style="float:right" ng-click="displaymyads()">See all my advertisements</a></h2>
					<div><hr></div>
					
				
					<div class="col-sm-6" ng-show="myads" ng-repeat="product in products"> 
						<div class="panel panel-primary">
							<div class="panel-heading" align="middle" style="font-size:auto" ng-model="product_name_model"><strong><h3>{{product.product_name}}</h3></strong></div>
							<div class="panel-body" style="height:100px">
								<div class="row" align="middle" ng-model="product_desc_model"><strong>{{product.product_description}}</strong></div>
								<br>
							</div>	
						
							<div class="panel-footer">
								<div class="row">
									<div class="col-sm-4" align="middle" ng-model="product_price_model"> Price/Item: ${{product.product_price}} </div>
					
									<div class="col-sm-4" align="middle" ng-model="product_qty_model"> Quantity: {{product.product_qty}}</div>
					
									<div class="col-sm-4" align="middle" ng-model="product_seller_model"> Seller: {{product.seller_uname}}</div>
								</div>
							</div>	
						</div>
					</div>
					

				</div> 
				<div class="col-sm-2"> </div>  <!--right empty column-->
			</div> <!--FIFTH MAIN ROW OR LAST ROW for Today's Featured Collections-->
			
			
			
			<div class="row"> 
				<div class="col-sm-2"> </div> 
				
				<div class="col-sm-8"> 
					<h2>Sales history ({{soldresultlength}})<a style="float:right" ng-click="solditems()">See all sold items </a></h2>
					<div><hr></div>
					
				
					<div class="col-sm-6" ng-show="sold_items" ng-repeat="sproduct in soldproducts"> 
						<div class="panel panel-primary">
							<div class="panel-heading" align="middle" style="font-size:auto" ng-model="product_name_model"><strong><h3>{{sproduct.product_name}}</h3></strong></div>
							<div class="panel-body" style="height:100px">
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Bill ID: {{sproduct.bill_id}}</strong></div>
								
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Total Price: ${{sproduct.totalamt}}</strong></div>
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Total Items: {{sproduct.totalitems}}</strong></div>
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Buyer info: {{sproduct.buyer_name}}</strong></div>
								<br>
							</div>	
						
							<div class="panel-footer">
								<div class="row">
									<div class="col-sm-4" align="middle" ng-model="product_price_model"> Price/Item: ${{sproduct.product_price}} </div>
					
									<div class="col-sm-4" align="middle" ng-model="product_qty_model"> Quantity: {{sproduct.product_qty}}</div>
					
									<div class="col-sm-4" align="middle" ng-model="product_seller_model"> Paid using: {{sproduct.bill_payment}}</div>
								</div>
							</div>	
						</div>
					</div>
					

				</div> 
				<div class="col-sm-2"> </div>
			</div> 	
			
			
			
			
			<div class="row"> 
				<div class="col-sm-2"> </div> 
				
				<div class="col-sm-8"> 
					<h2>Purchase history ({{purchasedresultlength}})<a style="float:right" ng-click="purchaseditems()">See all purchased items </a></h2>
					<div><hr></div>
					
				
					<div class="col-sm-6" ng-show="purchased_items" ng-repeat="pproduct in purchasedproducts"> 
						<div class="panel panel-primary">
							<div class="panel-heading" align="middle" style="font-size:auto" ng-model="product_name_model"><strong><h3>{{pproduct.product_name}}</h3></strong></div>
							<div class="panel-body" style="height:100px">
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Bill ID: {{pproduct.bill_id}}</strong></div>
								
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Total Price: ${{pproduct.totalamt}}</strong></div>
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Total Items: {{pproduct.totalitems}}</strong></div>
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Seller: {{pproduct.seller_name}}</strong></div>
								<br>
							</div>	
						
							<div class="panel-footer">
								<div class="row">
									<div class="col-sm-4" align="middle" ng-model="product_price_model"> Price/Item: ${{pproduct.product_price}} </div>
					
									<div class="col-sm-4" align="middle" ng-model="product_qty_model"> Quantity: {{pproduct.product_qty}}</div>
					
									<div class="col-sm-4" align="middle" ng-model="product_seller_model"> Paid using: {{pproduct.bill_payment}}</div>
								</div>
							</div>	
						</div>
					</div>
					

				</div> 
				<div class="col-sm-2"> </div>
			</div> 		


			<div class="row"> 
				<div class="col-sm-2"> </div> 
				
				<div class="col-sm-8"> 
					<h2>Bidding history ({{bidresultlength}})<a style="float:right" ng-click="biditems()">See all bid items </a></h2>
					<div><hr></div>
					
				
					<div class="col-sm-6" ng-show="bid_items" ng-repeat="bproduct in bidproducts"> 
						<div class="panel panel-primary">
							<div class="panel-heading" align="middle" style="font-size:auto" ng-model="product_name_model"><strong><h3>{{bproduct.product_name}}</h3></strong></div>
							<div class="panel-body" style="height:100px">
								<div class="row" align="middle" ng-model="product_desc_model"><strong>{{bproduct.product_desc}}</strong></div>
								<div class="row" align="middle" ng-model="product_desc_model"><strong>Bid ends at: {{bproduct.bid_endtime}}</strong></div>
								<div class="row" align="middle" ng-model="product_bidamt_model"><strong>Bid Amount: ${{bproduct.bid_price}} </strong></div>
								<br>
							</div>	
						
							<div class="panel-footer">
								<div class="row">

									<div class="col-sm-4" align="middle" ng-model="product_qty_model"> Quantity: {{bproduct.product_qty}}</div>
					
									<div class="col-sm-8" align="middle" ng-model="product_seller_model">Seller: {{bproduct.product_seller}}</div>
								</div>
							</div>	
						</div>
					</div>
					

				</div> 
				<div class="col-sm-2"> </div>
			</div> 			
			
			
			
			
	
			
			
			
	
			
			
			
			

	
		</body>
